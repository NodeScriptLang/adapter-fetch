{
    "name": "@nodescript/fetch-service",
    "version": "2.4.1",
    "description": "Fetch Service for sending HTTP requests",
    "type": "module",
    "sideEffects": false,
    "private": true,
    "scripts": {
        "clean": "rm -rf out *.tsbuildinfo",
        "dev": "npm run compile && run-p dev:*",
        "dev:ts": "tsc -b -w",
        "dev:http": "NODE_ENV=development node ./out/bin/http.js",
        "compile": "npm run clean && tsc -b",
        "lint": "eslint --ext=.js,.ts,.vue --cache .",
        "test": "NODE_NO_WARNINGS=1 NODE_ENV=test mocha",
        "build": "run-s compile",
        "version": "run-s version:*",
        "version:compile": "npm run compile",
        "postversion": "git push --tags origin main"
    },
    "pre-commit": [
        "lint"
    ],
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/NodeScriptLang/fetch-service.git"
    },
    "keywords": [],
    "author": "Boris Okunskiy",
    "license": "ISC",
    "dependencies": {
        "@nodescript/errors": "^1.1.2",
        "@nodescript/http-server": "^1.6.0",
        "@nodescript/logger": "^2.0.1",
        "@nodescript/metrics": "^1.1.1",
        "@nodescript/microframework": "^1.7.0",
        "@nodescript/protocomm": "^1.0.0",
        "airtight": "^5.7.0",
        "dotenv": "^16.0.3",
        "mesh-config": "^1.0.0",
        "mesh-ioc": "^3.2.0",
        "undici": "^5.22.1"
    },
    "devDependencies": {
        "@nodescript/eslint-config": "^1.0.4",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.4.4",
        "eslint": "^8.34.0",
        "mocha": "^10.2.0",
        "npm-run-all": "^4.1.5",
        "pre-commit": "^1.2.2",
        "typescript": "^4.9.5"
    }
}
